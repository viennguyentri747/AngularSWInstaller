        "@angular/platform-browser-dynamic": "^17.3.0",
        "@angular/router": "^17.3.0",
        "@types/body-parser": "^1.19.5",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.12.7",
        "@types/unzipper": "^0.10.9",
        "axios": "^1.6.8",
        "cross-fetch": "^4.0.0",
        "fs-extra": "^11.2.0",
        "multer": "^1.4.5-lts.1",
