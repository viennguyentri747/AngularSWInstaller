{
    "name": "ng-sw-installer",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "bash -c \"./runner/update_config.sh\" && ng serve --configuration=production",
        "start_dev": "bash -c \"./runner/update_config.sh\" && ng serve --configuration=development --host=127.0.0.1",
        "build": "ng build",
        "watch": "ng build --watch --configuration=development",
        "test": "ng test",
        "start_server": "bash -c \"./runner/update_config.sh && ./runner/start_server.sh\""
    },
    "private": true,
    "dependencies": {
